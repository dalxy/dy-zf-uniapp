					"titleNView": false	//*******去掉页面的导航栏
				}
			}
		},
		{
			"path": "pages/list/list",
			"style": {
				"navigationBarTitleText": "list"
			}
		}
	    ,{
            "path" : "pages/account/account",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
		,{
		    "path" : "pages/cloud/cloud",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "",
		        "enablePullDownRefresh": false
		    }
		    
		}
